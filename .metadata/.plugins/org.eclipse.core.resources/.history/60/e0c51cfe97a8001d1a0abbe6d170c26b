package pageObjects;

import org.openqa.selenium.By;
import org.openqa.selenium.WebDriver;
import org.openqa.selenium.WebElement;

public class AllPages {
	
	public WebDriver driver;
	
  By firstname=By.xpath("//input[@placeholder='First Name']");
  By lastname=By.xpath("//input[@placeholder='Last Name']");
  By stateclick=By.xpath("//span[normalize-space()='State']");
  By stateselect=By.xpath("//span[normalize-space()='MAHARASHTRA']");
  By nextbtn = By.xpath("//button[@class='button button-next next-button button-red']");
  By valpg=By.xpath("//h3[normalize-space()='Select ID Type']");
  By pancard=By.xpath("//label[@for='mat-radio-2-input']//div[@class='mat-radio-label-content']");
  By aadhar=By.xpath("//label[@for='mat-radio-3-input']//div[@class='mat-radio-label-content']");
  By backbtn =By.xpath("//mat-icon[@aria-label='Back']");
  By nextbtn2=By.xpath("(//button[@class='button button-brand next-button'][normalize-space()='Next'])[1]");
  By validatepg=By.xpath("//input[@placeholder='Name on PAN Card']");
  By backbtn2=By.xpath("//button[normalize-space()='Back']");
  By upload=By.xpath("//span[@class='file-upload-caption']");
  By dob=By.xpath("//input[@placeholder='Date Of Birth']");
  By year=By.xpath("//button[@aria-label='Choose month and year']");
  By PanNo=By.xpath("//input[@id='mat-input-14']");
  By lastnext=By.xpath("//button[@class='button button-brand next-button ml-10'][normalize-space()='Next']");
  By toast=By.xpath("//span[normalize-space()='Image Is Required']");
  By next20=By.xpath("//button[@aria-label='Previous 20 years']");
  By yearselect=By.xpath("//td[@aria-label='1995']");
  By month=By.xpath("//div[normalize-space()='JAN']");
  By day=By.xpath("//div[normalize-space()='16']");
  By PANName=By.xpath("//input[@placeholder='Name on PAN Card']");
  By bck=By.xpath("//button[normalize-space()='Back']");
  By toast1=By.xpath("//span[normalize-space()='Image is non compliant to request/quality standard']");
  By toast2=By.xpath("//span[normalize-space()='Input details does not match with Id.']");
  By popup=By.xpath("//span[normalize-space()='PAN Card Details entered successfully']");
  By nextnext=By.xpath("//button[@class='skip cursor-pointer ng-star-inserted']");
  By validatewithdrawpg=By.xpath("//h3[normalize-space()='Withdraw Cash']");
  By toast3=By.xpath("//span[normalize-space()='Document is already verified by other user.']");
  By dl=By.xpath("//label[@for='mat-radio-4-input']//div[@class='mat-radio-label-content']");
  By DLname=By.xpath("//input[@placeholder='Name on Driving License']");
  By frontupload=By.xpath("//div[contains(text(),'Front')]");
  By backupload=By.xpath("//div[contains(text(),'Back')]");
  By aadharno=By.xpath("//input[@placeholder='Enter 12 Digit Aadhar Card number']");
  By aadharName=By.xpath("//input[@placeholder='Name on Aadhar Card']");
  
	public AllPages(WebDriver driver5) {
	// TODO Auto-generated constructor stub
		this.driver=driver5;
}
	
	public WebElement getAadharName()
	{
		return driver.findElement(aadharName);
	}
	public WebElement getAadharNo()
	{
		return driver.findElement(aadharno);
	}
	public WebElement getBackUpload()
	{
		return driver.findElement(backupload);
	}
	public WebElement getFrontUpload()
	{
		return driver.findElement(frontupload);
	}
	public WebElement getDLname()
	{
		return driver.findElement(DLname);
	}
	public WebElement getDL()
	{
		return driver.findElement(dl);
	}
	public WebElement getAadhaar()
	{
		return driver.findElement(aadhar);
	}
	public WebElement getValidatewithdrawPg()
	{
		return driver.findElement(validatewithdrawpg);
	}
	public WebElement getNextNext()
	{
		return driver.findElement(nextnext);
	}
	public WebElement getPopUp()
	{
		return driver.findElement(popup);
	}
	public WebElement getToast2()
	{
		return driver.findElement(toast2);
	}
	public WebElement getToast1()
	{
		return driver.findElement(toast1);
	}
	public WebElement getBCK()
	{
		return driver.findElement(bck);
	}
	public WebElement getPanName()
	{
		return driver.findElement(PANName);
	}
	
	public WebElement getDay()
	{
		return driver.findElement(day);
	}
	public WebElement getMonth()
	{
		return driver.findElement(month);
	}
	public WebElement getYearSelect()
	{
		return driver.findElement(yearselect);
	}
	public WebElement getNext20()
	{
		return driver.findElement(next20);
	}
	public WebElement getToast()
	{
		return driver.findElement(toast);
	}
	public WebElement getLastNext()
	{
		return driver.findElement(lastnext);
	}
	public WebElement getFirstName()
	{
		return driver.findElement(firstname);
	}
	public WebElement getLastName()
	{
		return driver.findElement(lastname);
	}
	public WebElement getStateClick()
	{
		return driver.findElement(stateclick);
	}
	public WebElement getStateSelect()
	{
		return driver.findElement(stateselect);
	}
	public WebElement getNextBtn()
	{
		return driver.findElement(nextbtn);
	}
	public WebElement getPanCard()
	{
		return driver.findElement(pancard);
	}
	public WebElement getBackbtn()
	{
		return driver.findElement(backbtn);
	}
	public WebElement getNextBtn2()
	{
		return driver.findElement(nextbtn2);
	}
	public WebElement getValidatepg()
	{
		return driver.findElement(validatepg);
	}
	public WebElement getBackbtn2()
	{
		return driver.findElement(backbtn2);
	}
	public WebElement getUpload()
	{
		return driver.findElement(upload);
	}
	public WebElement getValPg()
	{
		return driver.findElement(valpg);
	}
	public WebElement getDOB()
	{
		return driver.findElement(dob);
	}
	public WebElement getYear()
	{
		return driver.findElement(year);
	}
	public WebElement getPanNo()
	{
		return driver.findElement(PanNo);
	}
}
